				<string>/System/Library/Automator/Run Shell Script.action</string>
				<key>ActionName</key>
				<string>Shell-Skript ausführen</string>
				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>php -- "$@" &lt;&lt;'EOF'
&lt;?php
$user = $argv[1];
if (!$xml = @file_get_contents('http://www.bing.com/HPImageArchive.aspx?format=xml&amp;idx=0&amp;n=1&amp;mkt=en-US')) {
  die();
}
$match = [];
if (!preg_match('/&lt;urlBase&gt;(.+)&lt;\/urlBase&gt;/', $xml, $match)) {
  die();
}
$url = 'http://www.bing.com' . $match[1] . '_1920x1080.jpg';
$folder = '/Users/' . $user . '/Pictures/';
// find and delete any old images
$dir = dir($folder);
while ($file = $dir-&gt;read()) {
  if (preg_match('/BingWallpaper-[0-9a-f]{4}\.jpg$/', $file)) {
    @unlink($folder . $file);
  }
}
// MD5 is fine here, no security involved
$file = $folder . 'BingWallpaper-' . substr(md5(time()), 0, 4) . '.jpg';
$fp = fopen($file, 'w');
fwrite($fp, file_get_contents($url));
fclose($fp);
echo $file;
EOF</string>
